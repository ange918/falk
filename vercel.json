{
  "rewrites": [
    {
      "source": "/assets/(.*)",
      "destination": "/dist/public/assets/$1"
    },
    {
      "source": "/(.*)\\.(png|jpg|jpeg|svg|ico|json)",
      "destination": "/dist/public/$1.$2"
    },
    {
      "source": "/((?!assets/|.*\\..*).*)",
      "destination": "/dist/public/index.html"
    }
  ]
}
